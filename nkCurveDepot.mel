  }

  print("test4 start\n");
  string $r;

  // createCurve
  {
    string $xyzCurveFilename = $curveDirname + "/xyz.curvedepot";

    $r = assertFalse(existsFilename($curveOrderFilename)); if (`size $r`) warning $r;
